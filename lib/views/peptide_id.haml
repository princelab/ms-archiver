#content
	- category = "peptide_ids"
	- Ref_hash = { peptide_counts: "Peptide Counts", total_ion_current_for_ids_at_peak_maxima: "Total Ion Current for IDs at Peak Maxima", tryptic_peptide_counts: "Tryptic Peptide Counts", precursor_m_z_for_ids: "Precursor m/z for IDs", averages_vs_rt_for_ided_peptides: "Averages vs RT for IDed Peptides"}
	%h1 Testing this whole system out, I am	
	#overview
	- Dir.chdir('lib/public')
	- subcats = Dir.glob("#{category}/*/*.svg").map{|a| a.slice(/.*?\/(.*?)\/\w*?\.svg/,1)}.uniq.compact
	%ul
	- subcats.sort.each do |subcat|
		%li 
			=link_to("##{subcat}", subcat)
	#pictures
	- subcats.sort.each do |subcat|
		%h2{:id => subcat }= Ref_hash[subcat.to_sym]
		- @a = Dir.glob("#{category}/#{subcat}/*.svg")
		- @a.map do |a|
			=  img(a) 
		%p 

		= link_to("#overview", "Return to the top") 
