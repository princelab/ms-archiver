#content
	- category = "chromatography"
	- Ref_hash = {wide_rt_differences_for_ids_4_min: "Wide RT differences for IDs (> 4 min)", peak_widths_at_half_max_over_rt_deciles_for_ids: "Peak Widths at Half Max over RT, deciles for IDs", rt_ms1max_rt_ms2_for_ids_sec: "RT(MS1max)-RT(MS2) for IDs (seconds)", middle_peptide_retention_time_period_min: "Middle Peptide Retention Time Period (min)", max_peak_width_for_ids_sec: "Max Peak Width for IDs (seconds)", first_and_last_ms1_rt_min: "First and Last MS1 Retention Time in Minutes", peak_width_at_half_height_for_ids: "Peak Width at Half Height for IDs"}
	%h1 Testing this whole system out, I am	
	#overview
	- Dir.chdir('lib/public')
	- subcats = Dir.glob("#{category}/*/*.svg").map{|a| a.slice(/.*?\/(.*?)\/\w*?\.svg/,1)}.uniq.compact
	%ul
	- subcats.sort.each do |subcat|
		%li 
			=link_to("##{subcat}", Ref_hash[subcat.to_sym])
	#pictures
	- subcats.sort.each do |subcat|
		%h2{:id => subcat }= Ref_hash[subcat.to_sym]
		- @a = Dir.glob("#{category}/#{subcat}/*.svg")
		- @a.map do |a|
			=  img(a) 
		%p 

		= link_to("#overview", "Return to the top") 
